{
  "extends": "airbnb",
  "parser": "babel-eslint",
  
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "globals": {
    "window": false,
    "google": false,
    "document": false
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "arrowFunctions": true,
      "classes": true,
      "generators": true,
      "modules": true,
      "jsx": true
    }
  },
  
  "rules": {
    "react/prefer-stateless-function":0,
    "react/forbid-prop-types": 0,
    "react/require-default-props": 0,
    "jsx-a11y/label-has-for": 0,
    "jsx-a11y/anchor-is-valid": 0,
    "jsx-a11y/click-events-have-key-events": 0,
    "react/no-unescaped-entities": 0,
    "jsx-a11y/no-static-element-interactions": 0,
    "jsx-a11y/no-autofocus" : true, 
    "react/no-unused-state": 0,
    "react/no-unused-prop-types": 0,
    "no-unused-vars": 0,
    "no-else-return": 0,
    "object-shorthand": 0,
    "max-len": [2, 120, 2],
    "react/prop-types": 0,
    "no-underscore-dangle": 0,
    "react/jsx-indent":0,
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".jsx"]
      }
    ]
  }
}
